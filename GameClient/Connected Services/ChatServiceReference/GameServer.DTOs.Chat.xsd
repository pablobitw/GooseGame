<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:tns="http://schemas.datacontract.org/2004/07/GameServer.DTOs.Chat" elementFormDefault="qualified" targetNamespace="http://schemas.datacontract.org/2004/07/GameServer.DTOs.Chat" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:complexType name="JoinChatRequest">
    <xs:sequence>
      <xs:element minOccurs="0" name="LobbyCode" nillable="true" type="xs:string" />
      <xs:element minOccurs="0" name="Username" nillable="true" type="xs:string" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="JoinChatRequest" nillable="true" type="tns:JoinChatRequest" />
  <xs:simpleType name="ChatOperationResult">
    <xs:restriction base="xs:string">
      <xs:enumeration value="Success" />
      <xs:enumeration value="SpamBlocked" />
      <xs:enumeration value="ContentBlocked" />
      <xs:enumeration value="TargetNotFound" />
      <xs:enumeration value="LobbyNotFound" />
      <xs:enumeration value="MessageTooLong" />
      <xs:enumeration value="InternalError" />
      <xs:enumeration value="GeneralError" />
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="ChatOperationResult" nillable="true" type="tns:ChatOperationResult" />
  <xs:complexType name="ChatMessageDto">
    <xs:sequence>
      <xs:element minOccurs="0" name="IsPrivate" type="xs:boolean" />
      <xs:element minOccurs="0" name="LobbyCode" nillable="true" type="xs:string" />
      <xs:element minOccurs="0" name="Message" nillable="true" type="xs:string" />
      <xs:element minOccurs="0" name="Sender" nillable="true" type="xs:string" />
      <xs:element minOccurs="0" name="TargetUser" nillable="true" type="xs:string" />
      <xs:element minOccurs="0" name="Timestamp" type="xs:dateTime" />
    </xs:sequence>
  </xs:complexType>
  <xs:element name="ChatMessageDto" nillable="true" type="tns:ChatMessageDto" />
</xs:schema>